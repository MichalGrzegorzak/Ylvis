    <add xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)"
         key="ExtensionsBasePath" value="C:\tools\Lib"/>
    <add xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)"
             key="OutputFolderUrl" value="C:\results\_temp\Screenshots"/>
  </appSettings>
</configuration>