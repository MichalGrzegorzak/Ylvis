<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Stock.Core" namespace="Stock.Core.Domain">
    <class lazy="false" name="Company" table="Companies" >
    
    <id name="ID" type="System.String" column="Id" length="3">
        <generator class="assigned"/>
    </id>
    <property name="Name" column="Name" type="System.String" not-null="true" length="50"/>
    <property name="PaperCode" column="PaperCode" type="System.String" length="12"/>
 </class>
</hibernate-mapping>
