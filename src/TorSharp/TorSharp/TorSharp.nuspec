<?xml version="1.0"?>
<package >
  <metadata>
    <id>Knapcode.TorSharp</id>
    <version>1.7.0</version>
    <authors>Joel Verhagen</authors>
    <licenseUrl>https://raw.githubusercontent.com/joelverhagen/TorSharp/master/LICENSE</licenseUrl>
